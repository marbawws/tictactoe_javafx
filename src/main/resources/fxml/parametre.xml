<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.CheckBox?>
<VBox xmlns:fx="http://javafx.com/fxml" styleClass="conteneurPrincipal" VBox.vgrow="ALWAYS">
	<HBox  styleClass="conteneurTitre" >
		<Text text="%para"/>
	</HBox>
	<HBox VBox.vgrow="ALWAYS">
		<VBox styleClass="conteneursDifficulty"  HBox.hgrow="ALWAYS">
			<HBox VBox.vgrow="ALWAYS" styleClass="conteneurDifficulty">
			<Text text="%diff" />
			</HBox>
			<HBox VBox.vgrow="ALWAYS" styleClass="conteneurDifficulty">
			<CheckBox text="%fac" HBox.hgrow="ALWAYS"/>
			</HBox>
			<HBox VBox.vgrow="ALWAYS" styleClass="conteneurDifficulty">
			<CheckBox text="%moy" HBox.hgrow="ALWAYS"/>
			</HBox>
			<HBox VBox.vgrow="ALWAYS" styleClass="conteneurDifficulty">
			<CheckBox text="%impo" HBox.hgrow="ALWAYS"/>
			</HBox>
		</VBox>
	</HBox>
</VBox>